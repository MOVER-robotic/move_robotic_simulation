<?xml version="1.0"?>
<launch>
	<arg name="input_topic" default="imu0"/>
	<arg name="output_topic" default="imu0/fiter_out"/>
	<arg name="cutoff" default="1"/>

	<node pkg="move_robotic_simulation" type="lowpass_filter_node" name="lowpass_filter_node" output="screen">
		<param name="input_topic" value="$(arg input_topic)"/>
		<param name="output_topic" value="$(arg output_topic)"/>
		<param name="cutoff" value="$(arg cutoff)"/>
	</node>

</launch>
